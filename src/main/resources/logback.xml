<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <appender name="FILE" class="ch.qos.logback.core.FileAppender">
      <file>bininja-server.log</file>
      <encoder>
         <pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n
         </pattern>
      </encoder>
   </appender>

   <root level="all">
      <appender-ref ref="FILE" />
   </root>
</configuration>
